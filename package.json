{
  "name": "@omnibazaar/documentation",
  "version": "0.1.0",
  "description": "Decentralized AI-powered documentation and customer support system for OmniBazaar",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "documentation",
    "ai",
    "ipfs",
    "decentralized",
    "customer-support",
    "blockchain",
    "automation"
  ],
  "author": "OmniBazaar Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/omnibazaar/omnibazaar.git",
    "directory": "Documents"
  },
  "scripts": {
    "dev": "npm run dev:api",
    "dev:api": "nodemon --exec ts-node src/api/index.ts",
    "build": "npm run build:api",
    "build:api": "tsc -p tsconfig.build.json",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:coverage": "jest --coverage",
    "test:ai": "python -m pytest tests/ai/",
    "lint": "eslint src/**/*.{ts,tsx} --fix",
    "lint:check": "eslint src/**/*.{ts,tsx}",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,md}",
    "ipfs:start": "ipfs daemon",
    "ipfs:stop": "ipfs shutdown",
    "ipfs:status": "ipfs id",
    "ipfs:init": "node scripts/setup/init-ipfs.js",
    "setup:ipfs": "npm run ipfs:init && npm run ipfs:start",
    "ai:train": "python scripts/ai/train-models.py",
    "ai:validate": "python scripts/ai/validate-models.py",
    "ai:deploy": "node scripts/deployment/deploy-ai-models.js",
    "docs:generate": "typedoc src --out docs/api-reference/generated",
    "docs:extract": "node tools/extractors/extract-docs.js",
    "docs:validate": "node tools/validators/validate-docs.js",
    "content:sync": "node scripts/maintenance/sync-content.js",
    "content:backup": "node scripts/maintenance/backup-content.js",
    "content:restore": "node scripts/maintenance/restore-content.js",
    "deploy:dev": "node scripts/deployment/deploy-dev.js",
    "deploy:staging": "node scripts/deployment/deploy-staging.js",
    "deploy:prod": "node scripts/deployment/deploy-prod.js",
    "deploy:ipfs": "node scripts/deployment/deploy-ipfs.js",
    "setup:hooks": "husky install && node scripts/setup/setup-hooks.js",
    "config:notifications": "node scripts/setup/config-notifications.js",
    "dev:setup": "npm run setup:hooks && npm run ipfs:init",
    "db:migrate": "knex migrate:latest",
    "db:seed": "knex seed:run",
    "start": "node dist/api/index.js",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@apollo/client": "^4.0.5",
    "@fastify/cors": "^8.4.2",
    "@fastify/websocket": "^8.3.1",
    "@graphql-tools/schema": "^10.0.2",
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow/tfjs-node": "^4.15.0",
    "axios": "^1.5.1",
    "cross-fetch": "^4.1.0",
    "dotenv": "16.6.1",
    "express": "4.21.2",
    "fastify": "^4.24.3",
    "graphql": "^16.11.0",
    "graphql-ws": "^6.0.6",
    "ipfs-http-client": "^60.0.1",
    "knex": "^3.0.1",
    "libp2p": "^0.46.21",
    "mercurius": "^13.3.3",
    "onnxruntime-node": "1.21.0",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.6",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@playwright/test": "1.55.0",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.1",
    "@types/pg": "8.15.5",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "@types/ws": "8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "7.1.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.0",
    "eslint-plugin-jsdoc": "48.11.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.2",
    "prettier": "3.6.2",
    "style-loader": "4.0.0",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.4",
    "ts-node": "^10.9.1",
    "typedoc": "0.27.9",
    "typescript": "5.8.3",
    "webpack": "5.101.3",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "5.2.2"
  },
  "engines": {
    "node": ">=22.18.0",
    "npm": ">=10.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "src/**/*.{js,jsx,json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run docs:validate",
      "pre-push": "npm run test && npm run typecheck"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
